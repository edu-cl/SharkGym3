<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sharkgym3">

    <application
        android:name=".views.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".views.SearchActivity"
            android:label="@string/title_activity_search"
            android:theme="@style/Theme.SharkGym3.Search"></activity>
        <activity
            android:name=".views.AboutMe"
            android:label="@string/title_activity_about_me"
            android:theme="@style/Theme.SharkGym3.about" />
        <activity android:name=".views.FormActivity" />
        <activity
            android:name=".views.SplartActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SharkGym3.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        a
    </application>

</manifest>